{
  "expo": {
    "name": "anime-streaming",
    "slug": "anime-streaming",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/totoro.png",
    "splash": {
      "image": "./assets/totoro.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/totoro.png",
        "backgroundColor": "#FFFFFF"
      },
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/totoro.png"
    }
  }
}
